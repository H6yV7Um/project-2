/* pages/pubdPreview /pubdPreview .wxss */
.pubPreBg{width: 100%;height: 1280rpx;background-color: #f5eaed;overflow-x: hidden;overflow-y: auto}

.pubpRreimg{width: 96%;height: 240rpx;margin-left: 2%;margin-top: 10rpx;overflow: hidden}
.pubpRreimgL{width: 49%;height: 240rpx;background-color: white;display: inline-block;float: left;padding: 10rpx;box-sizing: border-box;}
.pubpRreimgR{width: 49%;height: 240rpx;background-color: white;display: inline-block;float: right;padding: 10rpx;box-sizing: border-box;}
.pubpRreimgBot{width: 100%;height: 50rpx;position: relative;top: -60rpx;background-color: black;opacity: 0.8;color: white;text-align: center;line-height: 50rpx;font-size: 30rpx;margin: 0;}
.pubpRreimgL image{width: 100%;height: 100%;}
.pubpRreimgR image{width: 100%;height: 100%;}