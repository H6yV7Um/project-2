/* pages/aboutUs/aboutUs.wxss */
.aboutUsBg{width: 100%;height: 1210rpx;background-color: #f4f0f1}
.usLogo{width: 100%;height: 420rpx;}
.usLogoImg{width: 160rpx;height: 160rpx;background-color: #ff648c;border-radius: 20rpx;position: relative;left: 50%;top:100rpx;margin-left: -80rpx;}
.usLogoW{width: 250rpx;height: 90rpx;position: relative;top: 100rpx;left: 50%;margin-left: -125rpx;text-align: center;margin-top:15rpx;}
.usLogoW1{font-size: 38rpx;font-weight: bold}
.usLogoW2{font-size: 32rpx;}
.callUs{width: 100%;height: 360rpx;background-color: white}
.callUscot{width: 80%;height: 120rpx;margin-left: 10%;border-bottom: 1px solid #f3eeee}
.callUscotL{display: inline-block;width: 40%;height: 100%;float: left}
.callUscotR{display: inline-block;width: 60%;height: 100%;float: right;line-height: 120rpx;font-size: 32rpx;color: #ffa001;font-weight: bold;text-align: right}
.callUscot image{width: 60rpx;height: 60rpx;margin-top: 32rpx;}
.callUscot label{position: relative;top: -18rpx;font-size: 30rpx;font-weight: bold;color: #525252}
.aboutUsFot{width: 100%;height: 150rpx;background-color: transparent;text-align: center;font-size: 36rpx;font-weight: bold;line-height: 150rpx;color: #535253;margin-top: 280rpx;}
