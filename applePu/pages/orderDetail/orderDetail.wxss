.unable{ background-color: #f3f3f3!important;
color:#bebebe!important;}
 .goToorder button{margin-top: 15rpx;margin-left: 50rpx;} 


.orderAddress{width: 96%;height: 35%;margin-left: 2%;margin-top: 20rpx;font-size: 30rpx;background-color: white;border-radius: 10rpx;padding: 40rpx;box-sizing: border-box}


.orderAddressTittle{font-size: 32rpx;font-weight: bold}
.orderAddressTittlecot{font-size: 22rpx;color: #979797}
.orderAddressName{font-size: 28rpx}
.orderAddressNamecot{font-size: 22rpx;color: #979797}
.orderAddressNamecotline{width: 100%;height: 2rpx;border-top: 2px dashed #e1e1e1;margin-top: 10rpx;}

.orderservices{width: 100%;margin-left: 0px;height: 80rpx;margin-top: 10rpx;}
.orderservicesL image{width: 50rpx;height: 50rpx; margin-top: 15rpx;}
.orderservicesL label{width: 80rpx;height: 80rpx;display: inline-block;top: -10rpx;position: relative;color: #a9a9a9}
.orderservicesR image{width: 50rpx;height: 50rpx; margin-top: 15rpx;}
.orderservicesR label{width: 80rpx;height: 80rpx;display: inline-block;top: -10rpx;position: relative;color: #a9a9a9;}
.orderservicesL{display: inline-block;width:50%;height: 100%;float: left;text-align: center;line-height: 80rpx;}
.orderservicesR{display: inline-block;width:40%;height: 100%;float: right;text-align: center;line-height: 80rpx;}
.orderservicesM{display: inline-block;width: 4rpx;height: 60rpx;background-color: #d3d3d3;margin-top: 10rpx;}

.orderInfor{width: 96%;height: 26%;margin-top: 20rpx;margin-left: 2%;color: #656565;font-size: 30rpx;padding: 40rpx;box-sizing: border-box;border-radius: 10rpx;background-color: white}
.orderInfor view{width: 100%;height: 40rpx;}
.orderInfor label{display: inline-block;width: 500rpx;height: 40rpx;float: right;margin-top: 0rpx;}


.orderFooter{width: 96%;height: auto;margin-left: 2%; padding: 20rpx;box-sizing: border-box;color: #a0a0a0;font-size: 28rpx;}
.call{width: 80%;height: 130rpx;margin-left: 10%;}
.call button{display: inline-block;width: 100%;height: 80rpx;line-height: 80rpx;background:#f36201;color: #ffa001;margin-top: 20rpx;}





