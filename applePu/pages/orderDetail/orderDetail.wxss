/* pages/messageL/messageL.wxss */
.container{
  font-family: "微软雅黑";
  padding: 0rpx 20rpx;
  padding-top: 1px;
  box-sizing: border-box;
}




.modal-line-box{
  margin: 30rpx 0rpx;
}




.mask{
  position: fixed;
  bottom: 120rpx;
  left: 0px;
  width: 100%;
  height:100%;
  z-index: 100;
  background:rgba(0,0,0,0.5);
}
.showMask,.showModal{
  display: block;
}
.hideMask,.hideModal{
  display: none;
}
.messageCale{width: 100%;height: 30rpx;}
.cale{width: 30rpx;height: 30rpx;font-size: 36rpx;line-height: 25rpx;color: #4f4e4e}
.evaluateTit{width: 100%;height: 40rpx;text-align: center;font-size: 36rpx}
